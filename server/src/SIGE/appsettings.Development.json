{
  "Urls": {
    "Development": "http://localhost:5000",
    "Staging": "https://0.0.0.0:5000",
    "Production": "https://0.0.0.0:5000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "Swagger": {
    "Path": "/swagger/v1/swagger.json",
    "Scope": "SIGE"
  },
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRING}"
  },
  "System": {
    "EnableSchedule": false,
    "DelaySchedule": 12,
    "JwtSecurityToken": "${JWT_SECURITY_TOKEN}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq", "Serilog.Filters.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "apiKey": "${SEQ_API_KEY}"
        }
      }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "RequestMethod = 'OPTIONS'" }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Services": {
    "Ccee": {
      "CertificateValue": "${CERTIFICATE_VALUE}",
      "CertificatePass": "${CERTIFICATE_PASS}",
      "BaseUrl": "https://servicos.ccee.org.br:443",
      "ListarMedidas": {
        "Url": "/ws/medc/ListarMedidaBSv1",
        "SoapAction": "listarMedida"
      },
      "ResultadoRelatorios": {
        "Url": "/ws/v2/ResultadoRelatorioBSv2",
        "SoapAction": "listarResultadoRelatorio"
      }
    }
  },
  "Email": {
    "Server": "mail.coenel-de.com.br",
    "Imap": null,
    "Port": 465,
    "ImapPort": 993,
    "SenderName": "Coenel-DE: Assessoria em Energia El√©trica",
    "SenderEmail": "contato@coenel-de.com.br",
    "UserName": "contato@coenel-de.com.br",
    "Password": "${EMAIL_PASSWORD}",
    "ContactPhone": "(54) 3452-7735 / (54) 99142-4241",
    "ContactMail": "nediane@coenel-de.com.br"
  },
  "Cache": {
    "AbsoluteExpiration": 30,
    "Expiration": 10,
    "Priority": 1,
    "Auth": {
      "MenuUsuario": {
        "Key": "MenuUsuario:{0}",
        "Expiration": 10
      }
    },
    "Dashboard": {
      "Checklist": {
        "Key": "Checklist:{0}",
        "Expiration": 10
      },
      "ConsumoMeses": {
        "Key": "ConsumoMeses:{0}",
        "Expiration": 10
      },
      "ContratosFinalizados": {
        "Key": "ContratosFinalizados:{0}",
        "Expiration": 10
      },
      "StatusMedicoes": {
        "Key": "StatusMedicoes:{0}",
        "Expiration": 10
      }
    }
  }
}
